<!DOCTYPE html>
<html>
  <head>
    <title>Outdoor Restaurant Reservations | OutdoorTable</title>
    <%= csrf_meta_tags %>
    <%= csp_meta_tag %>

    <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track': 'reload' %>
    <%= javascript_include_tag 'application', 'data-turbolinks-track': 'reload' %>

    <script type="text/javascript">
      window.googleAPIKey = "<%= Rails.application.credentials.google[:api_key] %>"
    </script>

    <%= javascript_include_tag "https://maps.googleapis.com/maps/api/js?key=#{Rails.application.credentials.google[:api_key]}"%>

    <%= favicon_link_tag asset_path('favicon.png') %>

    
  </head>

  <body>
    <%= yield %>
  </body>
</html>
